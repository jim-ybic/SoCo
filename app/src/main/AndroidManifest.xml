<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.soco.SoCoClient" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShowProgramActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.soco.SoCoClient.MainActivity" />
        </activity>
        <!--activity
            android:name=".ReadFileActivity"
            android:label="@string/title_activity_read_file"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.soco.SoCoClient.MainActivity" />
        </activity-->
        <activity
            android:name="com.soco.SoCoClient.CreateProgramActivity"
            android:label="@string/title_activity_create_program"
            android:parentActivityName="com.soco.SoCoClient.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.soco.SoCoClient.MainActivity" />
        </activity>
    </application>

</manifest>
