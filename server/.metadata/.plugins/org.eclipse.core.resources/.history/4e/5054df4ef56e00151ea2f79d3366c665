package com.soco.test.cases;

import java.io.File;

import com.soco.log.Log;

public class TestCaseFileManager {

	private static String TEST_CASE_FOLDER = "./";
	
	private void searchTestCaseFiles(){
		if(this.TEST_CASE_FOLDER != ""){
			File folder = new File(this.TEST_CASE_FOLDER);
			File[] listOfFiles = folder.listFiles();
			for(File file : listOfFiles){
				if(file.isFile()){
					Log.debug("File name: " + file.getName());
				} else if(file.isDirectory()){
					Log.debug("Folder name: " + file.getName());
				} else {
					
				}
			}
		}
	}
}
